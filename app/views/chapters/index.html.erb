<div class="page-header">
  <h1>Chapters</h1>
</div>

<div class="panel-group">
  <% @chapters.each do |chapter| %>
    <div class="panel panel-default">
    <div class="panel-heading">
      <h2>
        <%= link_to chapter do %>
          <%= chapter.name %>
        <% end %>
      </h2>
    </div>
    </div>
  <% end %>
</div>

<% content_for(:adminbar) do %>
  <li><%= link_to 'New Chapter', new_chapter_path %></li>
<% end %>
